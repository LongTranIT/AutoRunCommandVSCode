{
    "name": "autotuncommand",
    "displayName": "AutoRunCommand",
    "description": "Run command when open VSCode",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.81.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "AutoRunCommand.helloWorld",
                "title": "Hello World"
            },
            {
                "command": "long",
                "title": "long"
            }
        ],
        "configuration": {
            "title": "Auto Run Command",
            "properties": {
                "enableFeature": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable or disable a feature in My Extension"
                },
                "command": {
                    "type": "string",
                    "default": "npm start",
                    "description": "The command will be run",
                    "order": 0
                },
                "projectPath": {
                    "type": "string",
                    "default": "",
                    "description": "The project's path will be run\n(Run first project in workspace if empty)",
                    "order": 1
                },
                "addProject": {
                    "type": "boolean",
                    "default": false,
                    "description": "Add project path above to workspace",
                    "order": 2
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.81.0",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "20.2.5",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "eslint": "^8.41.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^5.1.3",
        "@vscode/test-electron": "^2.3.2"
    }
}
